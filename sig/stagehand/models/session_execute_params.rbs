      type agent_config =
        {
          cua: bool,
          model: Stagehand::ModelConfig,
          provider: Stagehand::Models::SessionExecuteParams::AgentConfig::provider,
          system_prompt: String
        }

      class AgentConfig < Stagehand::Internal::Type::BaseModel
